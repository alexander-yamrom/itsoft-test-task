# Server Configuration
PORT=3000
NODE_ENV=development

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/data-service-a

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@localhost:5672
RABBITMQ_EXCHANGE=data_service_events
RABBITMQ_QUEUE=data_service_logs
RABBITMQ_ROUTING_KEY=data.service.logs
# Additional RabbitMQ settings
RABBITMQ_USE_CONFIRM_CHANNEL=true
RABBITMQ_MAX_RECONNECT_ATTEMPTS=10
RABBITMQ_RECONNECT_DELAY_BASE=1000
RABBITMQ_PREFETCH_COUNT=10
RABBITMQ_ENABLE_DEAD_LETTER=true

# Logging Configuration
LOG_LEVEL=debug

# GeoDB Cities API Configuration
GEODB_API_BASE_URL=https://wft-geo-db.p.rapidapi.com/v1/geo/cities
GEODB_API_KEY=your_api_key_here
GEODB_API_RATE_LIMIT=10
GEODB_API_RATE_LIMIT_PERIOD=60 